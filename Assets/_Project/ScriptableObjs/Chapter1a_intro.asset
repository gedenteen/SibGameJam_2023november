  - upperText: "\u0420\u0430\u0441\u0441\u043A\u0430\u0437\u0447\u0438\u043A"
    mainText: "\u0413\u0435\u0440\u043E\u0439 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B\u0441\u044F
      \u0432 \u043F\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u0435.
      \u041E\u043D \u0432\u0437\u044F\u043B \u0432\u0441\u0435, \u0447\u0442\u043E
      \u043C\u043E\u0433\u043B\u043E \u0435\u043C\u0443 \u043F\u043E\u043C\u043E\u0447\u044C.
      \u0428\u043F\u0430\u0433\u0430, \u043A\u043E\u0442\u043E\u0440\u0430\u044F
      \u043E\u0441\u0442\u0430\u043B\u0430\u0441\u044C \u0435\u043C\u0443 \u043E\u0442
      \u043E\u0442\u0446\u0430, \u0438 \u0447\u0430\u0441\u044B, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
      \u0434\u043E\u0432\u0435\u0440\u0438\u043B\u0430 \u0435\u043C\u0443 \u043F\u0440\u0438\u043D\u0446\u0435\u0441\u0441\u0430."
