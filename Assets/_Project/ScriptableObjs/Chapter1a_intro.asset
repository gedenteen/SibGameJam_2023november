  - upperText: "\u0420\u0430\u0441\u0441\u043A\u0430\u0437\u0447\u0438\u043A"
    mainText: "\u0424\u0438\u043B\u0438\u043D \u043D\u0430\u043A\u043B\u043E\u043D\u044F\u0435\u0442\u0441\u044F
      \u0432\u043F\u0435\u0440\u0435\u0434, \u0434\u0430\u0432\u0430\u044F \u043C\u044B\u0448\u043E\u043A\u0443
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C \u043F\u0440\u0438\u0431\u043B\u0438\u0437\u0438\u0442\u044C\u0441\u044F
      \u043A \u0443\u0445\u0443. \u0413\u043E\u043B\u043E\u0432\u0430 \u0444\u0438\u043B\u0438\u043D\u0430
      \u0432 \u0442\u0440\u0438 \u0440\u0430\u0437\u0430 \u0431\u043E\u043B\u044C\u0448\u0435
      \u041C\u0443\u0441\u043A\u0430. \u041E\u043D \u0441 \u043B\u0435\u0433\u043A\u043E\u0441\u0442\u044C\u044E
      \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
      \u0438 \u043F\u0440\u043E\u0433\u043B\u043E\u0442\u0438\u0442\u044C \u0433\u0435\u0440\u043E\u044F
      \u0432 \u043E\u0434\u0438\u043D \u043C\u0438\u0433. \u041C\u0443\u0441\u043A
      \u0433\u0434\u0435-\u0442\u043E \u0441\u043B\u044B\u0448\u0430\u043B, \u0447\u0442\u043E
      \u0435\u0441\u043B\u0438 \u0432\u043E\u0442\u043A\u043D\u0443\u0442\u044C \u0448\u043F\u0430\u0433\u0443
      \u0432 \u0433\u043B\u0430\u0437 \u0438\u043B\u0438 \u0432 \u0443\u0445\u043E
      \u0431\u043E\u043B\u044C\u0448\u043E\u043C\u0443 \u0436\u0438\u0432\u043E\u0442\u043D\u043E\u043C\u0443,
      \u0442\u043E \u0435\u0441\u0442\u044C \u0448\u0430\u043D\u0441 \u0443\u0431\u0438\u0442\u044C
      \u0435\u0433\u043E \u043D\u0430 \u043C\u0435\u0441\u0442\u0435. \u041D\u043E
      \u0447\u0442\u043E \u0435\u0441\u043B\u0438 \u0444\u0430\u0440\u0442\u0443\u043D\u0430
      \u043D\u0435 \u0431\u0443\u0434\u0435\u0442 \u0431\u043B\u0430\u0433\u043E\u0441\u043A\u043B\u043E\u043D\u043D\u0430?"
