      \u0418 \u0432\u043E\u0442 \u043E\u043A\u0430\u0437\u0430\u0432\u0448\u0438\u0441\u044C
      \u043F\u043E\u0441\u0440\u0435\u0434\u0438 \u043A\u043E\u043C\u043D\u0430\u0442\u044B
      \u041C\u0443\u0441 \u043F\u043E\u0447\u0443\u0432\u0441\u0442\u0432\u043E\u0432\u0430\u043B
      \u043C\u0443\u0440\u0430\u0448\u043A\u0438 \u043F\u043E \u0441\u0432\u043E\u0435\u0439
