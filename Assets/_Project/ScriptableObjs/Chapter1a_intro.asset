  - upperText: "\u0420\u0430\u0441\u0441\u043A\u0430\u0437\u0447\u0438\u043A"
    mainText: "\u0412\u043E\u0437\u043C\u043E\u0436\u043D\u043E \u0436\u0438\u0437\u043D\u044C
      \u043E\u0442\u0432\u0435\u0442\u0438\u043B\u0430 \u043D\u0430 \u0432\u043E\u0439\u043D\u044B,
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E \u0442\u0430\u043A \u043F\u0440\u043E\u0441\u0442\u043E
      \u0441\u043B\u0443\u0447\u0438\u043B\u043E\u0441\u044C, \u043D\u043E \u0432
      \u043A\u043E\u0440\u043E\u043B\u0435\u0432\u0441\u0442\u0432\u043E \u043F\u0440\u0438\u0448\u043B\u0430
      \u0431\u043E\u043B\u0435\u0437\u043D\u044C."
    spentTimeInHours: 0
    backgroundImage: {fileID: 21300000, guid: 4991bc1ce48efc143b42003b7a852965, type: 3}
