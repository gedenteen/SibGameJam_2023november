  - upperText: "\u0420\u0430\u0441\u0441\u043A\u0430\u0437\u0447\u0438\u043A"
    mainText: "\u041D\u043E \u0432\u0441\u0435 \u043D\u0435 \u043C\u043E\u0433\u043B\u043E
      \u0431\u044B\u0442\u044C \u043D\u0430\u0441\u0442\u043E\u043B\u044C\u043A\u043E
      \u043F\u043B\u043E\u0445\u043E. \u0414\u0430\u0436\u0435 \u0432 \u0441\u0430\u043C\u043E\u0435
      \u0442\u0435\u043C\u043D\u043E\u0435 \u0432\u0440\u0435\u043C\u044F \u043C\u043E\u0436\u0435\u0442
      \u043F\u043E\u044F\u0432\u0438\u0442\u044C\u0441\u044F \u0442\u043E\u0442,
      \u043A\u0442\u043E \u0440\u0430\u0437\u0432\u0435\u0435\u0442 \u0442\u044C\u043C\u0443."
    spentTimeInHours: 0
    backgroundImage: {fileID: 0}
    spritesOfCharsToShow: []
    changeCharsToShow: 0
    musicToPlay: {fileID: 0}
  - upperText: "\u0420\u0430\u0441\u0441\u043A\u0430\u0437\u0447\u0438\u043A"
    mainText: "\u0411\u044B\u043B \u0433\u0435\u0440\u043E\u0439, \u0441\u0435\u0440\u0434\u0446\u0435,
      \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E \u0431\u044B\u043B\u043E \u0434\u043E\u0431\u0440\u044B\u043C
      \u0438 \u0447\u0443\u0442\u043A\u0438\u043C \u043A \u043F\u0440\u043E\u0431\u043B\u0435\u043C\u0430\u043C
      \u0434\u0440\u0443\u0433\u0438\u0445 \u043C\u044B\u0448\u0435\u0439. \u0418
      \u043A\u043E\u0433\u0434\u0430 \u0431\u043E\u043B\u0435\u0437\u043D\u044C \u043A\u043E\u0441\u043D\u0443\u043B\u0430\u0441\u044C
      \u043A\u043E\u0440\u043E\u043B\u044F, \u043E\u043D \u043D\u0435 \u043E\u0441\u0442\u0430\u043B\u0441\u044F
      \u0432 \u0441\u0442\u043E\u0440\u043E\u043D\u0435."
    spentTimeInHours: 0
    backgroundImage: {fileID: 0}
    spritesOfCharsToShow: []
